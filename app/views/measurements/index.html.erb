<p id="notice"><%= notice %></p>

<h1>Listing Measurements</h1>

<table>
  <thead>
    <tr>
      <th>Station</th>
      <th>Valuetype</th>
      <th>Measuredat</th>
      <th>Measuredvalue</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= link_to "an article", measurements_path(import: true) %>
  <tbody>
    <% @measurements.each do |measurement| %>
      <tr>
        <td><%= measurement.station.name %></td>
        <td><%= measurement.value_type.name %></td>
        <td><%= measurement.measured_at %></td>
        <td><%= measurement.measured_value %></td>
        <td><%= measurement.active %></td>
        <td><%= link_to 'Show', measurement %></td>
        <td><%= link_to 'Edit', edit_measurement_path(measurement) %></td>
        <td><%= link_to 'Destroy', measurement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Measurement', new_measurement_path %>
